<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/item_cl_cancel"
    android:background="@drawable/border_gray_9a_fillshadow_round_8"
    android:layout_marginEnd="12dp"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content">

    <TextView
        android:id="@+id/item_myreport_cancel_cafename"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginTop="19dp"
        android:layout_marginBottom="19dp"
        android:fontFamily="@font/roboto_bold"
        android:letterSpacing="-0.02"
        android:text="현빈스빈스카페"
        android:textColor="@color/white_f6f5f2"
        android:textSize="12dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/item_myreport_cancel_date"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="26dp"
        android:layout_marginEnd="16dp"
        android:fontFamily="@font/roboto_regular"
        android:letterSpacing="-0.03"
        android:text="2020.11.30"
        android:textColor="@color/white_f6f5f2"
        android:textSize="8dp"
        app:layout_constraintBottom_toBottomOf="@+id/item_myreport_cancel_cafename"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/item_myreport_cancel_cafename"
        app:layout_constraintTop_toTopOf="@+id/item_myreport_cancel_cafename" />
</androidx.constraintlayout.widget.ConstraintLayout>